<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>ear</Title>
			<Shortcut>ear</Shortcut>
			<Description></Description>
			<Author></Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>var</ID>
					<ToolTip>Property type</ToolTip>
					<Default>var</Default>
				</Literal>
				<Literal>
					<ID>method</ID>
					<ToolTip>Property type</ToolTip>
					<Default>method</Default>
				</Literal>
				<Literal>
					<ID>retval</ID>
					<ToolTip>Property type</ToolTip>
					<Default>retval</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp">
<![CDATA[$var$.ExpectAndReturn("$method$", $retval$);]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
